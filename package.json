  },
  "homepage": "https://github.com/tywi6665/recipe_extension#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.17.1",
    "puppeteer": "^2.1.1",
    "socket.io": "^2.3.0",
    "url-parse": "^1.4.7"
  }
}
